{
	"extends": "@ichess/typescript-config/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./*"],
			"@server/*": ["./server/*"]
		}
	},
	"include": [
		"next-env.d.ts",
		"./src/**/*.ts",
		"./src/**/*.tsx",
		".next/types",
		".next/types/**/*.ts"
	],
	"exclude": ["node_modules"]
}
